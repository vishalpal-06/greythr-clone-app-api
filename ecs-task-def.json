{
  "family": "greythr-dev-task-definition",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::681526277210:role/aws-service-role/ecs.amazonaws.com/AWSServiceRoleForECS",
  "containerDefinitions": [
    {
      "name": "greythr-dev-con",
      "image": "REPLACED_BY_CI",
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
